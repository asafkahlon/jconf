{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "enable",
    "queues",
    "id",
    "pid_path",
    "logging"
  ],
  "properties": {
    "enable": {
      "type": "boolean",
      "default": false
    },
    "queues": {
      "type": "object",
      "required": [
        "rx",
        "tx"
      ],
      "properties": {
        "rx": {
          "type": "integer",
          "default": 0,
          "minimum": 10,
          "maximum": 10000
        },
        "tx": {
          "type": "integer",
          "default": 0,
          "minimum": 10,
          "maximum": 10000
        }
      }
    },
    "id": {
      "type": "integer",
      "default": 0
    },
    "pid_path": {
      "type": "string"
    },
    "logging": {
      "type": "object",
      "required": [
        "level"
      ],
      "properties": {
        "level": {
          "type": "string",
          "enum": [
            "debug",
            "info",
            "warning",
            "error",
            "critical"
          ],
          "default": "info"
        }
      }
    }
  }
}